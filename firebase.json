{
  "functions": {
    "source": ".output/server",
    "runtime": "nodejs18"
  },
  "hosting": {
    "site": "nuxt3-supabase-tryout",
    "public": ".output/public",
    "cleanUrls": true,
    "rewrites": [
      { 
        "source": "**", 
        "function": "server",
        "region": "asia-northeast1"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
